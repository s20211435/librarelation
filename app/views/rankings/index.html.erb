<p id="notice"><%= notice %></p>

<h1>Rankings</h1>

<table>
  <thead>
    <tr>
      <th>Isbn</th>
      <th>書籍名</th>
      <th>著者名</th>
      <th>順位</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rankings.each do |ranking| %>
      <tr>
        <td><%= ranking.isbn_number %></td>
        <td><%= ranking.title %></td>
        <td><%= ranking.author_name %></td>
        <td><%= ranking.book_rank %></td>
        <td><%= link_to '詳細', ranking %></td>
        <td><%= link_to '編集', edit_ranking_path(ranking) %></td>
        <td><%= link_to '削除', ranking, method: :delete, data: { confirm: '本当に削除しますか？' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to '新規作成', new_ranking_path %>
